  },
  "dependencies": {
    "@types/jest": "^26.0.3",
    "eslint-config-vtex": "^12.6.0",
    "moment": "^2.29.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.3",
    "ts-jest": "^26.1.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.5"
