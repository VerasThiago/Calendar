  "bugs": {
    "url": "https://github.com/VerasThiago/Calendar/issues"
  },
  "dependencies": {
    "@types/jest": "^26.0.3",
    "moment": "^2.29.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.3",
    "ts-jest": "^26.1.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.3"
